{
  "name": "portfolioserver",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "start": "ts-node ./src/server.ts",
    "postinstall": "tsc",
    "watch-node": "nodemon dist/src/server.js",
    "watch-ts": "tsc -w",
    "deploy": "git add . && git commit -m Heroku && git push heroku master"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "author": "Dennis Aleksandrov",
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.8",
    "@types/node": "^14.11.8",
    "aws-sdk": "^2.713.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.9.23",
    "nodemon": "^2.0.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/mongoose": "^5.7.31",
    "tslint": "^6.1.2",
    "typescript": "^3.9.6"
  },
  "engines": {
    "node": "v12.14.1"
  }
}
